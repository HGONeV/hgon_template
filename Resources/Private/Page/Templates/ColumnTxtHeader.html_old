<html xmlns:f="http://typo3.org/ns/TYPO3/CMS/Fluid/ViewHelpers" data-namespace-typo3-fluid="true">

<f:layout name="HgonDefault" />

<f:section name="Main">

    <f:section name="Header">
        <f:render partial="Layout/Header" arguments="{_all}" />
    </f:section>

    <f:render partial="Layout/ContentHeaderText" arguments="{_all}" section="parentPageContent" />

    <div class="main__content">

        <f:cObject typoscriptObjectPath="lib.content.render" data="{colPos:0}" />

    </div>

    <f:render partial="Layout/MainFooter" arguments="{_all}" />

</f:section>

</html>