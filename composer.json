{
    "name": "hgon/hgon-template",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Maximilian Fäßler",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5 || ^10.4 || ^11.5 || ^12.4 || ^13.4"
    },
    "autoload": {
        "psr-4": {
            "HGON\\HgonTemplate\\": "Classes"
        }
    },
